local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()
local Window = Rayfield:CreateWindow({
	Name = "CyberHub - Anime Adventure",
	LoadingTitle = "CyberHub",
	LoadingSubtitle = "by iiTaha",
	ConfigurationSaving = {
		Enabled = true,
		FolderName = nil, -- Create a custom folder for your hub/game
		FileName = "CyberHub"
	},
        Discord = {
        	Enabled = false,
        	Invite = "sirius", -- The Discord invite code, do not include discord.gg/
        	RememberJoins = true -- Set this to false to make them join the discord every time they load it up
        },
	KeySystem = false, -- Set this to true to use our key system
	KeySettings = {
		Title = "Sirius Hub",
		Subtitle = "Key System",
		Note = "Join the discord (discord.gg/sirius)",
		FileName = "SiriusKey",
		SaveKey = true,
		GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
		Key = "Hello"
	}
})





-- Values

getgenv().Westraid = true
getgenv().autoticket = true
getgenv().Ignotscraft = true
getgenv().Infinitycastle = true
getgenv().Nextroom = true
getgenv().Autoleave = true
getgenv().Autowomb = true
getgenv().Autoparade = true
getgenv().Autothriller = true
getgenv().Autoerwin = true
getgenv().Autogojo = true











-- Raid Functions

function Autowestraid()
    spawn(function()
        while getgenv().Westraid == true do 
            local args = {
                [1] = "_lobbytemplate215"
            }
            game:GetService("ReplicatedStorage").endpoints.client_to_server.request_join_lobby:InvokeServer(unpack(args))
        end
    end)
end

-- Craft Functions

function Autoignotscraft()
    spawn(function()
        while getgenv().Autoparade == true do
            game:GetService("ReplicatedStorage").endpoints.client_to_server.request_join_lobby:InvokeServer("_lobbytemplate_event28",{["selected_key"] = "key_jjk_map"})
        end
    end)
end

-- ErwinBuff Functions

function Autouseerwin()
    spawn(function()
        while getgenv().Autoerwin == true do
            game:GetService("ReplicatedStorage").endpoints.client_to_server.use_active_attack:InvokeServer(workspace._UNITS.erwin)
            wait(5)
        end
    end)
end

-- GojoSkill Functions

function Autoskillgojo()
    spawn(function()
        while getgenv().Autogojo == true do
            game:GetService("ReplicatedStorage").endpoints.client_to_server.use_active_attack:InvokeServer(workspace._UNITS.gojo_evolved)
            wait(5)
        end
    end)
end

-- Thriller Functions

function Autojointhriller()
    spawn(function()
        while getgenv().Autothriller == true do
            game:GetService("ReplicatedStorage").endpoints.client_to_server.request_join_lobby:InvokeServer("_lobbytemplate_event330")
        end
    end)
end

-- Womb Functions

function Autojoinwomb()
    spawn(function()
        while getgenv().Autowomb == true do
             game:GetService("ReplicatedStorage").endpoints.client_to_server.request_join_lobby:InvokeServer("_lobbytemplate_event229",{["selected_key"] = "key_jjk_finger"})
        end
    end)
end

-- Parade Functions

function Autojoinparade()
    spawn(function()
        while getgenv().Autoparade == true do
             game:GetService("ReplicatedStorage").endpoints.client_to_server.request_join_lobby:InvokeServer("_lobbytemplate_event229",{["selected_key"] = "key_jjk_finger"})
        end
    end)
end

-- InfCastle Functions

function Autoinfcastle()
    spawn(function()
        while getgenv().Infinitycastle == true do
            getgenv().Infinitycastle = 0

            for i, v in pairs(game:GetService("Players")[game.Players.LocalPlayer.Name].PlayerGui.InfiniteTowerUI.LevelSelect.InfoFrame.LevelButtons:GetChildren()) do
                if v.Name == "FloorButton" then
                    if v.clear.Visible == false and v.Locked.Visible == false then
                        local room = string.split(v.Main.text.Text, " ")
                        getgenv().Infinitycastle = tonumber(room[2])
                    end
                end
            end
            
            task.wait(1.5)
            local args = {
                [1] = getgenv().Infinitycastle
            }
            
            game:GetService("ReplicatedStorage").endpoints.client_to_server.request_start_infinite_tower:InvokeServer(unpack(args))
            task.wait(5)
        end
    end)
end

-- auto next room

function Autonextroom()
        spawn(function()
            while getgenv().Nextroom == true do
                task.wait(8)
                local GameFinished = game:GetService("Workspace"):WaitForChild("_DATA"):WaitForChild("GameFinished")
                if GameFinished.Value == true then
                game:GetService("ReplicatedStorage").endpoints.client_to_server.request_start_infinite_tower_from_game:InvokeServer()
            end
        end
    end)
end

-- Autoleave Functions

function Autoleavegame()
    spawn(function()
        while getgenv().Autoleave == true do
            task.wait()
            coroutine.resume(coroutine.create(function()
                local GameFinished = game:GetService("Workspace"):WaitForChild("_DATA"):WaitForChild("GameFinished")
                GameFinished:GetPropertyChangedSignal("Value"):Connect(function()
                print("Changed", GameFinished.Value == true)
                if GameFinished.Value == true then
                    repeat task.wait() until  game:GetService("Players").LocalPlayer.PlayerGui.ResultsUI.Enabled == true
                    task.wait()
                    pcall(function() webhook() end)
                    print("next")
                    task.wait(2)
                    game:GetService("ReplicatedStorage").endpoints.client_to_server.teleport_back_to_lobby:InvokeServer()
                end
                end)
            end))
            wait(2)
        end
    end)
end


-- Taps

local RaidTab = Window:CreateTab("Auto Farm", 4483362458) -- Title, Image

local CraftTab = Window:CreateTab("Auto Craft", 4483362458) -- Title, Image

local InfCastle = Window:CreateTab("Inf Castle", 4483362458) -- Title, Image








-- Sections 

local Section = RaidTab:CreateSection("AutoFarm")

local Section = CraftTab:CreateSection("AutoCraft")

local Section = InfCastle:CreateSection("AutoInfCastle")




-- Buttons

local Button = RaidTab:CreateButton({
	Name = "Teleport To Lobby",
	Callback = function()
        placeid = 8304191830  --change this to the place id you want
        tp = game:GetService('TeleportService')
        
        for i,v in pairs (game.Players:GetChildren()) do
            tp:Teleport(placeid, v)
        end        
	end,
})

-- Erwin Buff Toggles

local Toggle = RaidTab:CreateToggle({
    Name = "Auto Erwin Buff",
    CurrentValue = false,
    Flag = "Autoerwin", 
    Callback = function(Value)
        getgenv().Autoerwin = Value
        Autouseerwin()
    end
})

